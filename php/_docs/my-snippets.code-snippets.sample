	"php-render-img": {
		"prefix": "php-render-img",
		"body": [
			"<?php",
			"render_img([",
			"    'src' => '$1',",
			"    'alt' => '$2',",
			"    'width' => '$3',",
			"    'height' => '$4'",
			"]);",
			"?>"
		],
		"description": "PHP render_img関数",
	},
